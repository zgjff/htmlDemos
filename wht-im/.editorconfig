# Editor configuration, see https://editorconfig.org
root = true

[*]
charset = utf-8
indent_style = tab # 缩进符使用的风格 （tab ｜ space)
tab_width = 4
insert_final_newline = true #行开始是不是清除空格
trim_trailing_whitespace = true #文件最后添加空行

[*.ts]
quote_type = single

[*.md]
max_line_length = off
trim_trailing_whitespace = false
